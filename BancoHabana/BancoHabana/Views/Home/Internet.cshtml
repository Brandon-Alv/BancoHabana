    <style>
        .foto {
            margin-left: 20%;
        }

        h3 {
            text-align: center;
            align-content: center;
        }
    </style>
    <title>Agua</title>
</head>

<body>
    <div id="Agua" data-role="page">
        <div data-role="header" data-position="fixed" data-theme="c">
            <h3>Internet</h3>
        </div>
        <form action="pagarinternet" method="post">
            <div data-role="content">
                <div>
                    <label>Monto</label>
                    <input type="text" value="@Session["montointernet"]" name="montointernet" id="montointernet" readonly="readonly">
                    <label>Fecha Límite</label>
                    <input type="text" name="Fechalimite" value="@Session["fechainternet"]" id="Fechalimite" readonly="readonly">
                </div>
                <br>
                <input type="submit" value="Pagar" />
                <a href="MenuServicios" data-role="button">Regresar</a>
            </div>
        </form>
    </div>
</body>


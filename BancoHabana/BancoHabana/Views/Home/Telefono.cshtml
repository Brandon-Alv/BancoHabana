
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <style>
        .foto {
            margin-left: 20%;
        }

        h3 {
            text-align: center;
            align-content: center;
        }
    </style>
    <title>Telefono</title>
    <script type="text/javascript" src="~/Scripts/jquery-2.1.3.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.mobile-1.4.5.js"></script>
    <link rel="stylesheet" href="~/Content/jquery.mobile-1.4.5.css">
    <link rel="stylesheet" href="~/Content/jquery.mobile.theme-1.4.5.css">
    <link rel="stylesheet" href="~/Content/temafsc_3.css">
    <link rel="stylesheet" href="~/Content/jquery.mobile.external-png-1.4.5.css">
    <link rel="stylesheet" href="~/Content/jquery.mobile.icons-1.4.5.css">
    <link rel="stylesheet" href="~/Content/jquery.mobile.inline-png-1.4.5.css">
    <link rel="stylesheet" href="~/Content/jquery.mobile.structure-1.4.5.css">
</head>
<body>
    <div id="Telefono" data-role="page">
        <div data-role="header" data-position="fixed" data-theme="c">
            <h3>Telefono</h3>
        </div>
        <div data-role="content">
            <div>
                <label>Consumo Kolbi</label>
                <input type="text" name="MontoAgua" value="@Session["consumokolbi"]" id="MontoAgua" readonly="readonly">
                <label>Consumo Movistar</label>
                <input type="text" name="Fechalimite" value="@Session["consumomovistar"]" id="Fechalimite" readonly="readonly">
                <label>Consumo Claro</label>
                <input type="text" name="Fechalimite" value="@Session["consumoclaro"]" id="Fechalimite" readonly="readonly">
                <label>Consumo Internet</label>
                <input type="text" name="Fechalimite" value="@Session["consumointernet"]" id="Fechalimite" readonly="readonly">
                <label>Monto a Pagar</label>
                <input type="text" name="Fechalimite" value="@Session["monto"]" id="Fechalimite" readonly="readonly">
            </div>
            <br>
            <a href="Menuservicios" data-role="button">Regresar</a>
        </div>
    </div>
</body>
</html>

           